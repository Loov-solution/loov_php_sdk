{
    "name": "loov/php-sdk",
    "description": "Online payment solution",
    "keywords": [
        "payment",
        "php sdk",
        "online payment"
      ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Youssouf Junior",
            "email": "randyahmedjunior@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "1.9.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Loov\\PhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Loov\\PhpSdk\\Tests\\": "tests"
        }
      },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
